#nop 满内力;
#alias fullneili {
    #var fullhp 0;
    #class fullneili open;
    #act {{你运功完毕，深深吸了口气，站了起来。|你现在内力接近圆满状态}} {yun recover; hpbrief;}; 
    #act {#%!{(\d{5}|.+(K|M))},%!{-?}%d,%d,%!{-?}%d,%d,%d} {
        #if {$fullhp == 1} {
            #if {%%5 >= %%4} {
                #showme <fca>内力已满;
                #class fullneili kill;
            };
            #else {dazuo max};
        };
    };
    #act {#%d,%d,%d,%d,%d,%d} {
        #if {%%1 > %%2} {yun heal; #delay 1 hpbrief};
        #elseif {%%4 > %%5} {yun inspire; #delay 1 hpbrief};
        #else {
            #unact {#%d,%d,%d,%d,%d,%d};
            #var fullhp 1;
            hpbrief;
        };
    };
    #class fullneili close;
    xq;
    hpbrief;
};


